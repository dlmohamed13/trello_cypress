{
  "name": "trelloapp",
  "version": "1.0.0",
  "author": "Filip Hric",
  "license": "ISC",
  "scripts": {
    "start": "grunt dev && node index.js",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:run-headed": "npx cypress run --headed",
    "cy:run-chrome": "npx cypress run --browser chrome",
    "cy:run-FM": "npx cypress run --spec cypress/integration/*",
    "delete-results": "rm -rf cypress/results/* || true",
    "cy:report": "npx mochawesome-merge cypress/results/*.json -o mochawesome-report/mochawesome_cypress.json && npx marge mochawesome-report/mochawesome_cypress.json",
    "all:report":"npx mochawesome-merge mochawesome-report/*.json -o mochawesome-report/combined_report.json && npx marge combined_report.json ",
    "report:delete": "rm -rf mochawesome-report/* || true",
    "cy:regression": "npm run delete-results && npm run report:delete && npm run cy:run;",
    "api_test": "mocha api_testing/api_test/*  --timeout 3000 --reporter mochawesome --reporter-option reportFilename=mochawesome_api ",
    "all_test":"npm run cy:regression; npm run api_test; npm run cy:report; npm run all:report "
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^1.4.0",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@cypress/code-coverage": "^3.8.7",
    "@types/node": "^14.14.11",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chai-moment-string": "^1.0.0",
    "clipboardy": "^2.3.0",
    "connect-busboy": "0.0.2",
    "connect-history-api-fallback": "^1.6.0",
    "cypress": "^7.1.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-real-events": "^1.1.0",
    "cypress-terminal-report": "^3.0.3",
    "cypress-xpath": "^1.6.2",
    "elasticsearch": "^16.7.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.1",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.3.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "jenkins": "^0.28.1",
    "json-server": "^0.16.1",
    "json-server-auth": "^1.2.1",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.20",
    "mocha": "^8.3.2",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "moment": "^2.27.0",
    "pg": "^8.6.0",
    "prettier": "^2.2.1",
    "require-uncached": "^2.0.0",
    "sass": "^1.26.8",
    "sendmail": "^1.6.1",
    "socket.io": "^2.3.0",
    "typescript": "^4.0.2",
    "vue": "^2.6.11",
    "vue-focus": "^0.1.1",
